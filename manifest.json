{
  "name": "Easy as pie tab",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Easy as pie simple new tab extension",
  "homepage_url": "https://andrekuznetcov.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Easy as pie tab",
    "default_popup": "src/app/popup.html"
  },
  "omnibox": { "keyword" : "easy" },
  "chrome_url_overrides": {
    "newtab": "src/app/newtab.html"
  },
  "web_accessible_resources": [
    "src/app/newtab.js"
  ],
  "permissions": [
    "geolocation",
    "https://fonts.googleapis.com/*",
    "https://cdnjs.cloudflare.com/*",
    "http://res.cloudinary.com/*",
    "https://fonts.gstatic.com/*",
    "http://api.openweathermap.org/*",
    "https://*/*"
  ]
}